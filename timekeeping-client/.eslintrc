{
    "extends": "airbnb",
    "globals": {
        "document": true,
        "fetch": true,
        },
    "rules": {
      "linebreak-style": 0,
      "import/extensions": [0, "always"],
      "jsx-a11y/no-static-element-interactions": [0, "always"],
      import/no-extraneous-dependencies: [error, { devDependencies: true }],
    }
}
